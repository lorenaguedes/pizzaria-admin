<?xml version='1.0' encoding='utf-8'?>
<widget id="pizzaria.admin" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PizzariaAdminApp</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="https://pedidos-pizzaria.glitch.me/*" />
    <allow-navigation href="https://pedidos-pizzaria.glitch.me/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <plugin name="cordova-plugin-camera" source="npm" />
    <preference name="AndroidInsecureFileModeEnabled" value="true" />
</widget>
